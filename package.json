{
  "name": "mc-gateway",
  "version": "0.0.1",
  "private": true,
  "description": "New Swagger API Project",
  "keywords": [],
  "author": "",
  "license": "",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node --inspect --nolazy app.js",
    "test": "npm run lint && mocha --reporter spec ./tests/*.js",
    "testx": "npm run lint && mocha --reporter xunit-file ./tests/*.js",
    "test:cov": "nyc mocha ./tests/*.js",
    "lint": "eslint ./**/*.js",
    "lint:fix": "eslint --fix ./**/*.js",
    "release": "standard-version"
  },
  "dependencies": {
    "@map-colonies/mc-logger": "^1.1.0",
    "@map-colonies/mc-probe": "^1.0.0",
    "config": "^3.3.1",
    "dns-packet": "^5.2.1",
    "dns-socket": "^4.2.1",
    "express": "^4.12.3",
    "http-proxy": "^1.16.2",
    "js-yaml": "^3.14.1",
    "kontainer-di": "^0.9.9",
    "memory-cache": "^0.2.0",
    "swagger-ui-express": "^4.1.4",
    "sync-request": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitlint": "^9.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "sinon": "^9.0.2",
    "standard-version": "^8.0.2",
    "xunit-file": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
